    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test2",
            "type": "debugpy",
            "request": "launch",
            "program": "./sample_project/fabric_workspace_items/data_profiling/data_profiling_processor_lakehouse.Notebook/notebook-content.py",
            "console": "integratedTerminal"
        },
        {
            "name": "publish",
            "type": "debugpy",
            "request": "launch",
            "module": "typer",
            "args": [
                "ingen_fab/cli.py",
                "run",
                "--fabric-environment",
                "development",
                "deploy-to-environment"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "compile-ddl-notebooks",
            "type": "debugpy",
            "request": "launch",
            "module": "typer",
            "args": [
                "ingen_fab/cli.py",
                "run",
                "compile-ddl-notebooks"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run Simple Notebook",
            "type": "debugpy",
            "request": "launch",
            "module": "typer",
            "args": [
                "ingen_fab/cli.py",
                "run",
                "run-simple-notebook"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run Pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "./ingen_fab/python_libs_tests/pyspark/lakehouse_utils/test_lakehouse_utils_pytest.py"
            ],
            "console": "integratedTerminal"
        },        
        {
            "name": "Run Package Ingest",
            "type": "debugpy",
            "request": "launch",
            "module": "ingen_fab.packages.flat_file_ingestion.test_compiled_notebooks",           
            "console": "integratedTerminal"
        }
    ]
}